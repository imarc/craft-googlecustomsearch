{
    "name": "imarc/craft-googlecustomsearch",
    "description": "A Craft plugin for integrating with Google's Custom Search (and Google's Site Search.)",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "craft3",
        "craftcms",
        "craft-plugin",
        "google custom search",
        "google site search"
    ],
    "support": {
        "docs": "https://github.com/imarc/craft-googlecustomsearch",
        "issues": "https://github.com/imarc/craft-googlecustomsearch/issues"
    },
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Dan Collins",
            "email": "dan@imarc.com"
        },
        {
            "name": "Jeff Turcotte",
            "email": "jeff@imarc.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
            "imarc\\googlecustomsearch\\": "src/"
        }
    },
    "extra": {
        "name": "Google Custom Search",
        "handle": "googlecustomsearch",
        "developer": "Imarc",
        "developerUrl": "https://www.imarc.com",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://github.com/imarc/craft-googlecustomsearch/blob/master/CHANGELOG.md"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "craftcms/rector": "dev-main"
    }
}
